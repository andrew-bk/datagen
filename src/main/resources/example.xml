
<example>

<csv file="C:\output.csv" delimeter="\t">  <!-- if file not specified, output to console, delimter is also optional, defaults to ',' -->

    <integer  name="ConstantIntegerDatum" constant="1"/>
    <integer  name="IntegerRangeDatum" from="0" to="10"/>
    <sequence name="IntegerSequenceDatum" start="0" step="10" />  <!-- step is optional, 1 by default -->
    <integer  name="IntegerSetDatum">1,5,3,8,23</integer>

</csv>


<json file="C:\output.txt">    <!-- by default, with file not specified, output to console -->
    <datum/> <!-- 1 or more -->
    <object> <!-- 0 or more -->
        ...
    </object>
</json>

<!-- can't use xml as a tag name-->



</example>